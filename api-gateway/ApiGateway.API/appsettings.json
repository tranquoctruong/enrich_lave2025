{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "Microsoft.AspNetCore.Authentication": "Debug",
            "Ocelot": "Debug"
        }
    },
    "AllowedHosts": "*",
    "Jwt": {
        "Key": "THIS_IS_YOUR_SECRET_KEY_MUST_BE_LONG",
        "Issuer": "MyAppIssuer",
        "Audience": "MyAppAudience",
        "AccessTokenExpiresMinutes": 15,
        "RefreshTokenExpiresDays": 7
    }
}
